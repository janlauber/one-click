{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pocketbase && modd",
    "backend": "cd ../pocketbase && ./pocketbase serve --publicDir=../frontend/build",
    "build": "vite build",
    "build:backend": "cd ../pocketbase && go build",
    "typegen": "pocketbase-typegen --db ../pocketbase/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.2.3",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "flowbite": "^2.3.0",
    "flowbite-svelte": "^0.44.24",
    "pocketbase": "^0.21.1",
    "postcss": "^8.4.36",
    "prettier": "^3.2.3",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "svelte-dnd-action": "^0.9.42",
    "svelte-preprocess": "^5.1.3",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  },
  "type": "module",
  "dependencies": {
    "@codemirror/lang-html": "^6.4.7",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@floating-ui/dom": "^1.6.3",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@webcontainer/api": "^1.1.4",
    "@xyflow/svelte": "^0.0.40",
    "chart.js": "^4.3.0",
    "diff": "^5.2.0",
    "diff2html": "^3.4.47",
    "js-yaml": "^4.1.0",
    "lucide-svelte": "^0.365.0",
    "monaco-editor": "^0.47.0",
    "pocketbase-typegen": "^1.2.1",
    "prismjs": "^1.29.0",
    "svelte-codemirror-editor": "^1.2.0",
    "svelte-french-toast": "^1.2.0",
    "svelte-icons-pack": "^2.1.1",
    "svelte-local-storage-store": "^0.6.4",
    "svelte-monaco": "^0.3.0",
    "svelte-purify": "^1.1.19"
  }
}
