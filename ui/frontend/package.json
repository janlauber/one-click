{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pocketbase && modd",
    "backend": "cd ../pocketbase && ./pocketbase serve --publicDir=../frontend/build",
    "build": "vite build",
    "build:backend": "cd ../pocketbase && go build",
    "typegen": "pocketbase-typegen --db ../pocketbase/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.27.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/marked": "^5.0.0",
    "@types/node": "^20.2.3",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "daisyui": "^3.9.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "flowbite": "^1.8.1",
    "flowbite-svelte": "^0.44.18",
    "flowbite-svelte-icons": "^0.4.4",
    "pocketbase": "^0.18.3",
    "postcss": "^8.4.21",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.1",
    "svelte-check": "^3.0.1",
    "svelte-dnd-action": "^0.9.31",
    "svelte-preprocess": "^5.0.1",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.2.8",
    "@picocss/pico": "^1.5.10",
    "@sveltejs/adapter-node": "^1.2.1",
    "@sveltejs/adapter-static": "^2.0.2",
    "@webcontainer/api": "^1.1.4",
    "chart.js": "^4.3.0",
    "diff": "^5.1.0",
    "diff2html": "^3.4.45",
    "js-yaml": "^4.1.0",
    "lucide-svelte": "^0.293.0",
    "marked": "^5.0.4",
    "pocketbase-typegen": "^1.2.1",
    "prismjs": "^1.29.0",
    "svelte-french-toast": "^1.2.0",
    "svelte-icons-pack": "^2.1.0",
    "svelte-local-storage-store": "^0.6.4",
    "svelte-particles": "^2.12.0",
    "svelvet": "^8.1.0",
    "tsparticles": "^2.12.0",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0"
  }
}
